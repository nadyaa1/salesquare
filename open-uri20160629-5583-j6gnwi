		var createBaseHref = function () {
		  	return hm.url.getProtocol() + '//' + hm.url.getHost() + hm.url.getLocalizationUrlPrefix() + '/products/';  
		};
		
		if($('base').length === 0) {
			var baseTag = $('<base href="#"/>');
	        $('head').append(baseTag);
		}

		var baseHref = createBaseHref();
		$('base').attr('href', baseHref);
	};
	
	// add or set base tag to get relative urls to work    
    hm.productlist.setBaseTag();
	
